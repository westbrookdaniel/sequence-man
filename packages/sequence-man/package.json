    "@types/react-dom": "^18.0.4",
    "@vitejs/plugin-react": "^1.3.2",
    "c8": "^7.11.3",
    "eslint": "^8.15.0",
    "eslint-config": "*",
    "happy-dom": "^5.0.0",
    "react": "^18.1.0",
    "tsconfig": "*",
    "tsup": "^5.10.1",
    "typescript": "^4.5.3",
    "vitest": "^0.14.2",
    "zustand": "^3.6.9"
  },
  "publishConfig": {
